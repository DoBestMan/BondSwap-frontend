#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../ethereum-private-key-to-address/bin/ethereum_private_key_to_address" "$@"
  ret=$?
else 
  node  "$basedir/../ethereum-private-key-to-address/bin/ethereum_private_key_to_address" "$@"
  ret=$?
fi
exit $ret
