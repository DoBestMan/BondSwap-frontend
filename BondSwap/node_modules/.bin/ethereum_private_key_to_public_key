#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../ethereum-private-key-to-public-key/bin/ethereum_private_key_to_public_key" "$@"
  ret=$?
else 
  node  "$basedir/../ethereum-private-key-to-public-key/bin/ethereum_private_key_to_public_key" "$@"
  ret=$?
fi
exit $ret
