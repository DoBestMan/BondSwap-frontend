#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../ethereum-public-key-to-address/bin/ethereum_public_key_to_address" "$@"
  ret=$?
else 
  node  "$basedir/../../../ethereum-public-key-to-address/bin/ethereum_public_key_to_address" "$@"
  ret=$?
fi
exit $ret
