    "rc-tabs": "~11.7.0",
    "rc-textarea": "~0.3.0",
    "rc-tooltip": "~5.1.0",
    "rc-tree": "~4.1.0",
    "rc-tree-select": "~4.3.0",
    "rc-trigger": "^5.2.1",
    "rc-upload": "~4.2.0-alpha.0",
    "rc-util": "^5.9.4",
    "scroll-into-view-if-needed": "^2.2.25",
    "warning": "^4.0.3"
